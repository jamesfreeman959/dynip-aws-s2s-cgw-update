# Customer Gateway Name
CGW_NAME=YourGatewayName

# IAM Roles Anywhere Configuration
# Replace these with your actual ARNs from the setup process
ROLESANYWHERE_CERTIFICATE_PATH=/certs/app-cert.pem
ROLESANYWHERE_PRIVATE_KEY_PATH=/certs/app-key.pem
ROLESANYWHERE_TRUST_ANCHOR_ARN=arn:aws:rolesanywhere:eu-west-1:123456789012:trust-anchor/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
ROLESANYWHERE_PROFILE_ARN=arn:aws:rolesanywhere:eu-west-1:123456789012:profile/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
ROLESANYWHERE_ROLE_ARN=arn:aws:iam::123456789012:role/dynip-cgw-updater-role

# AWS Region
AWS_REGION=eu-west-1
